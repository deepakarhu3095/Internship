     "text": [
      "Enter the number:5\n",
      "The Factorial of  5 is 120\n"
     ]
    }
   ],
   "source": [
    "fact = 1\n",
    "num = int (input(\"Enter the number:\"))\n",
    "org = num\n",
    "while num > 0:\n",
    "    fact = fact * num\n",
    "    num = num-1\n",
    "print (\"The Factorial of \", org, \"is\",fact)    "
   ]
